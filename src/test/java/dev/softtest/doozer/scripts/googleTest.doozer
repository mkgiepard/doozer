set "google.url" "https://www.google.com"

navigateTo "" "${google.url}"

click "By.xpath('//button[contains(.,\"Accept\")]')"
type "By.cssSelector('textarea')" "doozer"
click "By.xpath('//input[@value=\"Google Search\"]')"

takeScreenshot "" "search-doozer"

sendKeys "" "doozer"
takeScreenshot "" "doozer"
navigateBack
takeScreenshot "" "fileName=back-withOption"
navigateForward
takeScreenshot
click? "By.id('someid')"
refresh
takeScreenshot "" "refresh"
assertCurrentUrl? "" "https://www.google.com/"